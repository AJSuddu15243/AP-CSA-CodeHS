public class StringMethods
{
    public static void main(String[] args)
    {
        String animal = "mimic octopus";
        
        int mystery1 = animal.indexOf("oct");
        System.out.println("Mystery 1 value: " + mystery1);
        
        boolean mystery2 = animal.equals("lion fish");
        System.out.println("Mystery 2 value: " + mystery2);
        
        int mystery3 = animal.compareTo("sea snake");
        System.out.println("Mystery 3 value: " + mystery3);
    }
}
