public class School
{
    public static void determineClassmates(Student one, Student two)
    {
        if (one == two) {
            System.out.println("Same student");
        } else if (one.getTeacher().equals(two.getTeacher())) {
            System.out.println("Same class");
        } else {
            System.out.println("Different class");
        }
    }
}
